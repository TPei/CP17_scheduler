{
  "AWSEBDockerrunVersion": 2,
  "authentication": {
    "bucket": "BUCKET-NAME",
    "key": "config.json"
  },
  "containerDefinitions": [
  {
    "name": "web",
    "image": "pandemic-web",
    "essential": true,
    "memory": 1024,
    "portMappings": [
    {
      "hostPort": 80,
      "containerPort": 4567
    }
    ]
  },
  {
    "name": "sidekiq",
    "image": "pandemic-sidekiq",
    "essential": true,
    "memory": 1024
  }
  ]
}
